    "t = time.strftime('%H:%M:%S')\n",
    "hour = int(time.strftime('%H'))\n",
    "if hour>=12:\n",
    "    print(\"Good afternoon\")\n",
    "elif 17=<hour=<21:\n",
    "    print(\"Good evening\")\n",
    "elif 21=<hour=<24 or 0=<hour=<4:\n",
    "    print(\"Good night\")\n",
    "else:\n",
    "    print(\"Good morning\")"
